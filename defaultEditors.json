[
    {
        "name": "@myeditorcool/codeeditor",
        "extension": [
            ".js",
            ".jsx",
            ".css",
            ".html",
            ".xml",
            ".json",
            ".ts"
        ],
        "readAsText": true
    },
    {
        "name": "@myeditorcool/doceditor",
        "extension": ".doc.json",
        "emptyContent": "{\"blocks\": [{\"type\": \"paragraph\",\"data\": {\"text\": \"\"}}]}",
        "readAsText": true
    },
    {
        "name": "@myeditorcool/erEditor"
    },
    {
        "name": "@myeditorcool/imageeditor",
        "extension": [
            ".png",
            ".jpg",
            ".jpeg"
        ]
    },
    {
        "name": "@myeditorcool/sheeteditor",
        "extension": ".sheet.json",
        "emptyContent": "{\"name\": \"Sheet1\",\"color\": \"\",\"index\": 1,\"status\": 0,\"order\": 1,\"celldata\": [],\"config\": {}}",
        "readAsText": true
    },
    {
        "name": "@myeditorcool/tableEditor"
    }
]